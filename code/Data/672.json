            "OutcomesModule": {
                "PrimaryOutcomeList": {
                    "PrimaryOutcome": [
                        {
                            "PrimaryOutcomeMeasure": "Increase of ER Protein Expression in ER-Negative/Low Breast Cancer",
                            "PrimaryOutcomeDescription": "To evaluate in a clinical neoadjuvant model whether MEK inhibitor AZD6244 can increase ER protein expression in ER-negative/low breast cancer, as measured by the \"ER response rate\" by both standard immunohistochemistry and Allred Score.",
                            "PrimaryOutcomeTimeFrame": "2 years"
                        }
                    ]
                },
