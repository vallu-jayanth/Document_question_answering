            "ArmsInterventionsModule": {
                "InterventionList": {
                    "Intervention": [
                        {
                            "InterventionType": "Other",
                            "InterventionName": "Blood samples",
                            "InterventionDescription": "Blood samples (3 ethylenediaminetetraacetic acid (EDTA) tubes with a volume of 7.5 ml blood each) will be processed for CTC isolation and characterization, ranging from molecular analysis to culture and drug screen."
                        },
                        {
                            "InterventionType": "Other",
                            "InterventionName": "Samples from tumor draining vessels",
                            "InterventionDescription": "In case of tumor resection, 3 EDTA tubes (5 ml) will be withdrawn from the tumor draining vessels for CTC isolation and characterization, ranging from molecular analysis to culture and drug screen."
                        }
                    ]
                }
            },
