            "ArmsInterventionsModule": {
                "ArmGroupList": {
                    "ArmGroup": [
                        {
                            "ArmGroupLabel": "Adults with Advanced Lung Cancer",
                            "ArmGroupType": "Experimental",
                            "ArmGroupDescription": "Older adults with stage III or stage IV lung cancer will be enrolled in the Self-System Therapy intervention to treat illness-related distress, depression, and to enhance self-efficacy via delivery of behavioral coping skills across a period of 10 weeks.",
                            "ArmGroupInterventionList": {
                                "ArmGroupInterventionName": [
                                    "Behavioral: Self System Therapy for Older Adults with Advanced Lung Cancer (SST-LC) Resilience"
                                ]
                            }
                        }
                    ]
                },
                "InterventionList": {
                    "Intervention": [
                        {
                            "InterventionType": "Behavioral",
                            "InterventionName": "Self System Therapy for Older Adults with Advanced Lung Cancer (SST-LC) Resilience",
                            "InterventionDescription": "The SST-LC is a brief structured intervention deliverable by video conference. The SST-LC Resilience protocol will incorporate behavioral strategies designed to meet the needs of older adults with lung cancer, including examining individual and shared expectations for lung cancer care, identifying discrepancies between expectations and realistic possibilities, and modifying stress-exacerbating behaviors. The focus will be to test measures of resilience during the intervention period.",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "Adults with Advanced Lung Cancer"
                                ]
                            }
                        }
                    ]
                }
            },
            "OutcomesModule": {
                "PrimaryOutcomeList": {
                    "PrimaryOutcome": [
                        {
                            "PrimaryOutcomeMeasure": "Change in Behavior as measured by the Cancer Behavior Inventory",
                            "PrimaryOutcomeDescription": "The Cancer Behavior Inventory measure will be used for rating confidence in behavior. The measure consist of 12 items. Scores range from 12-108 with higher scores indicating greater confidence.",
                            "PrimaryOutcomeTimeFrame": "Up to 10 weeks"
                        }
                    ]
                },
                "SecondaryOutcomeList": {
                    "SecondaryOutcome": [
                        {
                            "SecondaryOutcomeMeasure": "Change in Stress as measured by the Brief COPE",
                            "SecondaryOutcomeDescription": "The Brief COPE measure will be used for rating coping skills related to stress. The measure consists of 28 items. Scores range from 28-112 with higher scores indicating lower stress.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Resilience as measured by the CD-RISC-25",
                            "SecondaryOutcomeDescription": "The Connor-Davidson Resilience Scale (CD-RISC-10) will be used for measuring resilience. The measure consists of 25 items. Scores range from 0-40, with higher scores being an indicator of high resilience.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks."
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Depression as measured by the Beck Depression Inventory (BDI-II)",
                            "SecondaryOutcomeDescription": "Depression will be measured using the Beck Depression Inventory (BDI-II). The measure consists of 21 items. The BDIII is scored by summing the ratings for the 21 items. Each item is rated on a 4-point scale ranging from 0 to 3. The maximum total score is 63. Higher scores indicate greater depression.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks."
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Positive and Negative Affect as measured by the PANAS",
                            "SecondaryOutcomeDescription": "The Positive and Negative Scale (PANAS) measure will be used for rating to measure both positive and negative affect. The measure consists of two 10-item scales. Scores range from 10-50, with higher scores indicating higher positive and negative affect.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks."
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Physical Activity as measured by the Physical Activity Scale for the Elderly",
                            "SecondaryOutcomeDescription": "Physical Activity will be measured using the Physical Activity Scale for the Elderly (PASE)The instrument is comprised of self-reported occupational, household and leisure activities items over a one-week period. PASE scores are calculated from weights and frequency values for each of 12 types of activity.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Pain Interference as measured by the PROMIS PAI",
                            "SecondaryOutcomeDescription": "The Pain Interference PROMIS Scale measure will be used for rating pain interference in daily living. The measure consists of 6 items. Scores range from 6-30, with higher scores indicating higher pain interference.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Change in Quality of Life as measured by the QUALE",
                            "SecondaryOutcomeDescription": "The Quality-of-Life measure will be used for rating quality of life. The measure consists of 26 items. Scores range from 26-130, with higher scores indicating higher quality of life.",
                            "SecondaryOutcomeTimeFrame": "Up to 10 weeks"
                        }
                    ]
                }
            },
