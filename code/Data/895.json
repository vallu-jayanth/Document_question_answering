                "InterventionList": {
                    "Intervention": [
                        {
                            "InterventionType": "Behavioral",
                            "InterventionName": "Shared Decision Making for Colorectal Cancer Screening",
                            "InterventionDescription": "Age-eligible adults in the intervention communities will be exposed to the shared decision-making intervention when they are seen in the primary care clinics (N=all patients 50-75 years seen during the study period who are non-adherent to CRCS recommendation). Additionally, among patients with primary care visits scheduled one week or more before the visit, they will be randomized to receive either a mailed decision aid booklet or an informational flyer on shared decision making and CRCS prior to the visit. Patients with primary care visits scheduled less than one week prior to the visit will not be mailed materials in advance. The effect of the pre-visit materials on referral to SDM session and CRCS adherence between these groups will be compared. They will also be exposed to shared decision-making tools and resources available through the community-wide intervention activities.",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "Shared Decision Making"
                                ]
                            },
                            "InterventionOtherNameList": {
                                "InterventionOtherName": [
                                    "CRCS-WISDM"
                                ]
                            }
                        }
                    ]
                }
            },
            "OutcomesModule": {
                "PrimaryOutcomeList": {
                    "PrimaryOutcome": [
                        {
                            "PrimaryOutcomeMeasure": "Change in colorectal cancer screening adherence",
                            "PrimaryOutcomeDescription": "Colorectal cancer screening adherence will be defined as having screening according to the recommendations of the United States Preventive Services Task Force. Participants will be classified as screened per recommendations if they have had fecal occult blood testing in the last year, flexible sigmoidoscopy in the last five years, or colonoscopy in the last ten years. Participants will be classified as non-adherent if they have not had any of the modalities within the recommended timeframe.",
                            "PrimaryOutcomeTimeFrame": "Baseline up to 24 months post intervention initiation"
                        }
                    ]
                },
                "SecondaryOutcomeList": {
                    "SecondaryOutcome": [
                        {
                            "SecondaryOutcomeMeasure": "Decisional conflict",
                            "SecondaryOutcomeDescription": "A previously validated decisional conflict scale, will be used, which includes 16 items all measured on a 5-point Likert-type scale. (e.g. difficulty in decision making, certainty with a decision, demonstrated feeling of understanding about risks and benefits of a decision, and satisfaction with a decision). Responses will be coded so that higher responses represent greater decisional conflict.",
                            "SecondaryOutcomeTimeFrame": "Baseline up to 24 months post intervention initiation"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Modality-specific colorectal cancer screening barriers",
                            "SecondaryOutcomeDescription": "A previously validated modality-specific colorectal cancer screening barrier instrument will be used. The FOBT-specific scale includes 19 questions relating to barriers for fecal occult blood test and the colonoscopy-specific scale includes 21 questions relating barriers to colonoscopy. All items are measured on a 5-point Likert-type scale, with 5 representing greater barrier endorsement.",
                            "SecondaryOutcomeTimeFrame": "Baseline up to 24 months post intervention initiation"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Colorectal cancer screening-related confusion",
                            "SecondaryOutcomeDescription": "Colorectal cancer-related confusion will be measured using 9 items which will be coded on a scale from 1 to 5 where 5 represents greater confusion endorsement.",
                            "SecondaryOutcomeTimeFrame": "Baseline up to 24 months post intervention initiation"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Patients' shared decision-making experience",
                            "SecondaryOutcomeDescription": "Patients will answer multiple questions relating to their experience with the implementation of shared decision-making and their interactions with clinical staff. Items will be measured on Likert-like scales and will be coded so that higher scale numbers reflect more endorsement of positive opinions and experiences.",
                            "SecondaryOutcomeTimeFrame": "Baseline up to 12 months post intervention initiation"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Providers'/staff shared decision-making experience",
                            "SecondaryOutcomeDescription": "Providers and staff will answer multiple questions relating to their experience with the implementation of shared decision-making with their patients in clinical practice. Items will be measured on Likert-like scales and will be coded so that higher scale numbers reflect more endorsement of positive opinions and experiences.",
                            "SecondaryOutcomeTimeFrame": "Baseline up to 24 months post intervention initiation"
                        }
                    ]
                }
            },
            "EligibilityModule": {
                "EligibilityCriteria": "Inclusion Criteria:\n\n50 to 75 years of age\nLives in or receives care in the selected intervention or comparison (control) communities\nAverage-risk for colorectal cancer\nNon-adherent to CRCS recommendation\nEnglish-speaking\nPeople who consent to participate\n\nExclusion Criteria:\n\n<50 years of age or >75 years of age\nNot living in or receiving care in the selected intervention or comparison communities\nHigh-risk for colorectal cancer\nAdherent to CRCS recommendation\nNon-English speaking\nPeople who do not consent to participate",
