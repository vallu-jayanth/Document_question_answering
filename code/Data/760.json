                "InterventionList": {
                    "Intervention": [
                        {
                            "InterventionType": "Behavioral",
                            "InterventionName": "Questionnaires/Interviews",
                            "InterventionDescription": "Baseline assessment of distressed caregivers within 4-6 weeks a new visit to the thoracic clinic. Three-month follow-up assessment of caregivers. Qualitative phone interviews with a subsample of 30 caregivers within 3 weeks of follow-up.",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "family caregivers of lung cancer patients"
                                ]
                            }
                        }
