                "InterventionList": {
                    "Intervention": [
                        {
                            "InterventionType": "Radiation",
                            "InterventionName": "Short Course Radiotherapy",
                            "InterventionDescription": "Radiotherapy to tumor and draining lymph node with 25 Gy in 5 fractions within 5 working days",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "SCRT/ChemoTx with Delayed Surgery"
                                ]
                            },
                            "InterventionOtherNameList": {
                                "InterventionOtherName": [
                                    "Upfront Short-Course Radiotherapy"
                                ]
                            }
                        },
                        {
                            "InterventionType": "Drug",
                            "InterventionName": "Chemotherapy",
                            "InterventionDescription": "Oxaliplatin 85 mg/m2 IV over 2 hrs on Day 1\nIrinotecan 180 mg/m2 IV over 30-90 mins on Day 1\nLeucovorin 400 mg/m2 IV over 2 hrs on Day 1 and 2\n5-fluorouracil bolus 400 mg/m2 IV push on Day 1 and 2 (or 5-fluorouracil infusion 600 mg/m2 IV continuous infusion over 22 hrs).\nBevacizumab 5 mg/kg IV over 90 mins on Day 1\nCetuximab (only for patients with K-ras wild type and positive EGFR mutation) 400 mg/m2 IV over 2 hrs on Day 1, and 250 mg/m2 IV over 1 hr on Day 8, 15, 22, 29, and 36.\nFOLFOX or FOLFIRI (+-Bevacizumab ) repeats every 14 days for up to 3 courses. Cetuximab repeats every week for up to 6 courses\nPostoperative FOLFOX or FOLFIRI (+-Bevacizumab or Cetuximab) for up to 9 cycles (total 12 cycles)",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "SCRT/ChemoTx with Delayed Surgery"
                                ]
                            },
                            "InterventionOtherNameList": {
                                "InterventionOtherName": [
                                    "FOLFOX or FOLFIRI (+-Bevacizumab or Cetuximab)"
                                ]
                            }
                        },
                        {
                            "InterventionType": "Procedure",
                            "InterventionName": "Delayed Surgery",
                            "InterventionDescription": "If primary tumor and metastases is resectable after FOLFOX or FOLFIRI (+-Bevacizumab or Cetuximab) of 3 cycles, patients have surgical resection (and/or radiofrequency ablation to metastases).",
                            "InterventionArmGroupLabelList": {
                                "InterventionArmGroupLabel": [
                                    "SCRT/ChemoTx with Delayed Surgery"
                                ]
                            },
                            "InterventionOtherNameList": {
                                "InterventionOtherName": [
                                    "Total (or Tumor-specific) mesorectal excision"
                                ]
                            }
                        }
                    ]
                }
