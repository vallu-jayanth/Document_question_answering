            "OutcomesModule": {
                "PrimaryOutcomeList": {
                    "PrimaryOutcome": [
                        {
                            "PrimaryOutcomeMeasure": "Ability of genetically profiling of ptDNA as assessed by identifying the proportion of genetic alterations in tumor tissue",
                            "PrimaryOutcomeDescription": "The proportion of genetic alterations in tumor tissue is detected via genetic profiling of ptDNA when those genetic alterations are present in the tumor at an allelic frequency >10%.",
                            "PrimaryOutcomeTimeFrame": "1 year"
                        },
                        {
                            "PrimaryOutcomeMeasure": "Percentage of patients who cannot have NGS of metastatic site biopsy but have clinically actionable mutations detected via genetic profiling of ptDNA",
                            "PrimaryOutcomeDescription": "Percentage of patients for which genetic profiling of ptDNA would be more feasible when a metastatic biopsy cannot be acquired.",
                            "PrimaryOutcomeTimeFrame": "1 year"
                        }
                    ]
                },
                "SecondaryOutcomeList": {
                    "SecondaryOutcome": [
                        {
                            "SecondaryOutcomeMeasure": "Response as assessed by change in ptDNA level up to 2 weeks post-intervention. To determine whether a 10-fold decrease in allelic frequency of a given mutation in ptDNA after initiating new systemic therapy can predict for response to treatment.",
                            "SecondaryOutcomeDescription": "Change in number-of-folds decrease in allelic frequency of a given mutation in ptDNA 1 or 2 weeks after beginning new systemic therapy.",
                            "SecondaryOutcomeTimeFrame": "2 weeks"
                        },
                        {
                            "SecondaryOutcomeMeasure": "Response as assessed by Change in Circulating tumor cell (CTC) counts",
                            "SecondaryOutcomeDescription": "Measure CTC counts at baseline, after 1-2 weeks of therapy, and at each restaging, up to 1 year.",
                            "SecondaryOutcomeTimeFrame": "Change from baseline up to 1 year"
                        }
                    ]
                }
            },
